{
  "name": "es-beautifier",
  "description": "ECMAScript beautifier based on eslint",
  "version": "0.1.1",
  "repository": "dai-shi/es-beautifier",
  "main": "./lib/main.js",
  "bin": "./lib/cli.js",
  "scripts": {
    "lint": "eslint lib tests",
    "test": "npm run lint && npm run test:rules",
    "test:rules": "echo 'TODO'",
    "examples": "npm run ex:js-beautify && npm run ex:uglifyjs && npm run ex:esformatter && npm run ex:es-beautifier",
    "ex:js-beautify": "js-beautify -s 2 -X < ${FILE:-'examples/a.js'} && echo",
    "ex:uglifyjs": "uglifyjs -b indent-level=2 < ${FILE:-'examples/a.js'}",
    "ex:esformatter": "esformatter < ${FILE:-'examples/a.js'}",
    "ex:es-beautifier": "./lib/cli.js < ${FILE:-'examples/a.js'}"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "eslint": "^3.9.1"
  },
  "devDependencies": {
    "esformatter": "^0.10.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "js-beautify": "^1.6.4",
    "uglify-js": "mishoo/UglifyJS2.git#harmony"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "global-require": 0,
      "no-console": 0
    }
  },
  "keywords": [
    "eslint",
    "beautifier",
    "beautify"
  ],
  "license": "MIT"
}
