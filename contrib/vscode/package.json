{
  "name": "vscode-es-beautifier",
  "displayName": "es-beautifier",
  "publisher": "dai-shi",
  "description": "vscode plugin for es-beautifier",
  "icon": "logo1.svg",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/es-beautifier.git"
  },
  "dependencies": {
    "es-beautifier": "latest"
  },
  "devDependencies": {
    "vscode": "^1.0.3"
  },
  "activationEvents": [
    "onCommand:extension.esBeautifier"
  ],
  "main": "./index.js",
  "contributes": {
    "commands": [{
      "command": "extension.esBeautifier",
      "title": "es-beautifier"
    }]
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "javascript",
    "emcascript",
    "eslint",
    "beautifier",
    "beautify"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "eslintConfig": {
    "settings": {
      "import/core-modules": [
        "vscode"
      ]
    }
  },
  "license": "MIT"
}
